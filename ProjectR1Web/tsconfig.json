{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "src/",
    "paths": {
      "@component/*": [
        "app/component/*"
      ],
      "@model/*": [
        "app/model/*"
      ],
      "@service/*": [
        "app/service/*"
      ],
      "@utiity/*": [
        "app/utility/*"
      ],
      "@module/*": [
        "app/module/*"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  }
}